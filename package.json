{
  "name": "jest-supertest",
  "version": "0.0.0",
  "description": "jest-supertest",
  "main": "app.js",
  "author": {
    "name": ""
  },
  "scripts": {
    "test": "jest",
    "test-jenkins": "NODE_ENV=test jest --colors"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "verbose": true,
    "testResultsProcessor": "jest-jenkins-reporter"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "report.xml",
    "indent": 4
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "http-status-codes": "^1.4.0",
    "jest": "^25.1.0",
    "jest-jenkins-reporter": "^1.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3",
    "ws": "^7.2.1"
  }
}
